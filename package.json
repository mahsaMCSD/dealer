{
  "name": "auction-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/app",
  "scripts": {
    "build-css": "sass src/assets/styles/style.scss src/assets/css/style.css",
    "watch-css": "sass --watch src/assets/styles:src/assets/css",
    "react-start": "npm run build-css && env-cmd -f .env react-scripts start",
    "start": "npm-run-all --parallel react-start watch-css",
    "build-sw": "node src/sw-build.js",
    "build:staging": "npm run build-css && env-cmd -f .env.staging react-scripts build && npm run build-sw",
    "build:minor": "npm run build-css && env-cmd -f .env.minor react-scripts build && npm run build-sw",
    "build:impalaTechnical": "npm run build-css && env-cmd -f .env.impalaTechnical react-scripts build && npm run build-sw",
    "build:gocTechnical": "npm run build-css && env-cmd -f .env.gocTechnical react-scripts build && npm run build-sw",
    "build:fordTechnical": "npm run build-css && env-cmd -f .env.fordTechnical react-scripts build && npm run build-sw",
    "build:teslaTechnical": "npm run build-css && env-cmd -f .env.teslaTechnical react-scripts build && npm run build-sw",
    "build:technical": "npm run build-css && env-cmd -f .env.technical react-scripts build && npm run build-sw",
    "build:production": "npm run build-css && env-cmd -f .env.production react-scripts build && npm run build-sw",
    "lint": "eslint --ignore-path lint-files/.eslintignore -c lint-files/.eslintrc.json ./",
    "lint-fix": "npm run lint -- --fix",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@sentry/react": "6.0.0",
    "@sentry/tracing": "6.0.0",
    "amplitude-js": "7.3.3",
    "axios": "^0.27.2",
    "bootstrap": "5.1.3",
    "compressorjs": "^1.1.1",
    "env-cmd": "10.1.0",
    "events": "^3.3.0",
    "leaflet": "^1.8.0",
    "history": "^5.3.0",
    "moment-jalaali": "0.9.1",
    "prop-types": "15.7.2",
    "rc-slider": "^10.0.0",
    "react": "16.12.0",
    "react-circular-progressbar": "2.0.3",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.12.0",
    "react-easy-crop": "^4.2.0",
    "react-image-gallery": "^1.2.8",
    "react-leaflet": "^2.8.0",
    "react-modal": "3.11.2",
    "react-number-format": "^4.8.0",
    "react-pullable": "1.0.5",
    "react-rating-stars-component": "2.2.0",
    "react-redux": "7.1.3",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "react-scroll-manager": "^1.0.3",
    "react-share": "4.3.1",
    "react-start": "^1.1.2",
    "react-transition-group": "^4.4.2",
    "react-zoom-pan-pinch": "1.6.1",
    "redux": "4.0.4",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "redux-devtools-extension": "2.13.8",
    "sass": "^1.49.7",
    "workbox-build": "^6.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "./lint-files/.prettierrc.js"
}
