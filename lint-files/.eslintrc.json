{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "jest",
    "prettier"
  ],
  "rules": {
    "react/display-name": 0,
    "react/prop-types": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "semi": 0,
    "object-curly-spacing": 0,
    "linebreak-style": 0,
    "eol-last": 0,
    "no-else-return": 0,
    "no-shadow": 0,
    "import/no-unresolved": 0,
    "no-multi-spaces": 0,
    "import/order": 0,
    "function-paren-newline": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "arrow-body-style": 0,
    "indent": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "prefer-const": 0,
    "no-extend-native": 0,
    "space-before-function-paren": 0,
    "no-multiple-empty-lines": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "no-param-reassign": 0,
    "no-restricted-globals": 0,
    "no-use-before-define": 0,
    "prefer-template": 0,
    "quotes": 0,
    "no-mixed-operators": 0,
    "radix": 0,
    "spaced-comment": 0,
    "comma-dangle": 0,
    "no-restricted-syntax": 0,
    "camelcase": 0,
    "key-spacing": 0,
    "switch-colon-spacing": 0,
    "no-cond-assign": 0,
    "wrap-iife": 0,
    "no-restricted-properties": 0,
    "prefer-exponentiation-operator": 0,
    "eqeqeq": 0,
    "no-console": 0,
    "comma-spacing": 0,
    "guard-for-in": 0,
    "padded-blocks": 0,
    "default-case": 0,
    "operator-linebreak": 0,
    "implicit-arrow-linebreak": 0,
    "arrow-parens": 0,
    "object-curly-newline": 0,
    "quote-props": 0,
    "class-methods-use-this": 0,
    "no-prototype-builtins": 0,
    "no-continue": 0,
    "no-unreachable": 0,
    "consistent-return": 0,
    "no-useless-escape": 0,
    "operator-assignment": 0,
    "no-plusplus": 0,
    "react/jsx-filename-extension": 0,
    "no-undef": 0,
    "import/no-extraneous-dependencies": 0,
    "default-param-last": 0,
    "one-var": 0,
    "max-len": 0,
    "keyword-spacing": 0,
    "space-before-blocks": 0,
    "no-unneeded-ternary": 0,
    "object-property-newline": 0,
    "arrow-spacing": 0,
    "prefer-destructuring": 0,
    "react/jsx-tag-spacing": 0,
    "import/no-cycle": 0,
    "import/extensions": 0,
    "brace-style": 0,
    "array-callback-return": 0,
    "space-in-parens": 0,
    "object-shorthand": 0,
    "dot-notation": 0
  },
  "settings": {
    "jest": {
      "version": "latest"
    }
  }
}
